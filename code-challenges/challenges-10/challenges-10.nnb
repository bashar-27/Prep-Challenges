            "source": [
                "function birthdayCakeCandles(candles) {\n    let max=candles[0];\n    let count=1;\n    for(let i=1 ; i<=candles.length ; i++){\n        if(candles[i]>max){\n            //count=0;\n            max=candles[i];\n            count=1;\n        }\n        else if(candles[i]==max){\n            count++;\n        }\n    } \nreturn count;\n}"
